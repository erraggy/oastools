{
  "swagger": "2.0",
  "info": {
    "title": "Duplicate Operations Test API",
    "description": "A synthetic API spec to test duplicate operationId handling across tags. This tests: (1) multiple operations with the same operationId in different tags, (2) operations that normalize to the same Go method name, (3) file splitting behavior with duplicates.",
    "version": "1.0.0"
  },
  "host": "api.example.com",
  "basePath": "/v1",
  "schemes": ["https"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "tags": [
    {"name": "animals", "description": "Animal management"},
    {"name": "plants", "description": "Plant management"},
    {"name": "minerals", "description": "Mineral management"}
  ],
  "paths": {
    "/animals/items": {
      "get": {
        "tags": ["animals"],
        "operationId": "listItems",
        "summary": "List all animal items",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {"$ref": "#/definitions/ItemList"}
          }
        }
      },
      "post": {
        "tags": ["animals"],
        "operationId": "createItem",
        "summary": "Create an animal item",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {"$ref": "#/definitions/Item"}
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": {"$ref": "#/definitions/Item"}
          }
        }
      }
    },
    "/animals/items/{itemId}": {
      "get": {
        "tags": ["animals"],
        "operationId": "getItem",
        "summary": "Get an animal item by ID",
        "parameters": [
          {
            "name": "itemId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {"$ref": "#/definitions/Item"}
          }
        }
      },
      "delete": {
        "tags": ["animals"],
        "operationId": "deleteItem",
        "summary": "Delete an animal item",
        "parameters": [
          {
            "name": "itemId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "204": {
            "description": "Deleted"
          }
        }
      }
    },
    "/plants/items": {
      "get": {
        "tags": ["plants"],
        "operationId": "listItems",
        "summary": "List all plant items - DUPLICATE operationId",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {"$ref": "#/definitions/ItemList"}
          }
        }
      },
      "post": {
        "tags": ["plants"],
        "operationId": "createItem",
        "summary": "Create a plant item - DUPLICATE operationId",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {"$ref": "#/definitions/Item"}
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": {"$ref": "#/definitions/Item"}
          }
        }
      }
    },
    "/plants/items/{itemId}": {
      "get": {
        "tags": ["plants"],
        "operationId": "getItem",
        "summary": "Get a plant item by ID - DUPLICATE operationId",
        "parameters": [
          {
            "name": "itemId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {"$ref": "#/definitions/Item"}
          }
        }
      },
      "delete": {
        "tags": ["plants"],
        "operationId": "deleteItem",
        "summary": "Delete a plant item - DUPLICATE operationId",
        "parameters": [
          {
            "name": "itemId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "204": {
            "description": "Deleted"
          }
        }
      }
    },
    "/minerals/rocks": {
      "get": {
        "tags": ["minerals"],
        "operationId": "listRocks",
        "summary": "List all rocks",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {"$ref": "#/definitions/RockList"}
          }
        }
      }
    },
    "/minerals/gems": {
      "get": {
        "tags": ["minerals"],
        "operationId": "listGems",
        "summary": "List all gems",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {"$ref": "#/definitions/GemList"}
          }
        }
      }
    }
  },
  "definitions": {
    "Item": {
      "type": "object",
      "required": ["name"],
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier"
        },
        "name": {
          "type": "string",
          "description": "Item name"
        },
        "category": {
          "type": "string",
          "description": "Item category"
        }
      }
    },
    "ItemList": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
          "items": {"$ref": "#/definitions/Item"}
        },
        "total": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "Rock": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "mineralType": {
          "type": "string"
        },
        "hardness": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "RockList": {
      "type": "object",
      "properties": {
        "rocks": {
          "type": "array",
          "items": {"$ref": "#/definitions/Rock"}
        }
      }
    },
    "Gem": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "clarity": {
          "type": "string"
        },
        "carats": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "GemList": {
      "type": "object",
      "properties": {
        "gems": {
          "type": "array",
          "items": {"$ref": "#/definitions/Gem"}
        }
      }
    }
  }
}
